{
  "name": "markdown-syntax",
  "displayName": "Markdown Syntax",
  "description": "%description%",
  "version": "0.0.6",
  "publisher": "SeptWong",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "markdown",
    "md",
    "syntax",
    "example"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "markdown-syntax.openMarkdownWithSidePreview",
        "title": "%markdown-syntax.openMarkdownWithSidePreview.title%",
        "category": "Markdown syntax"
      },
      {
        "command": "markdown-syntax.mdTableToJson",
        "title": "%markdown-syntax.mdTableToJson.title%",
        "category": "Markdown syntax"
      },
      {
        "command": "markdown-syntax.jsonToMdTable",
        "title": "%markdown-syntax.jsonToMdTable.title%",
        "category": "Markdown syntax"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceExtname == .md",
          "command": "markdown-syntax.openMarkdownWithSidePreview",
          "group": "ms@1"
        },
        {
          "when": "editorTextFocus && editorLangId == markdown",
          "command": "markdown-syntax.mdTableToJson",
          "group": "ms@2"
        },
        {
          "when": "editorTextFocus && editorLangId == markdown",
          "command": "markdown-syntax.jsonToMdTable",
          "group": "ms@3"
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Septemberwh/markdown-syntax"
  },
  "bugs": {
    "url": "https://github.com/Septemberwh/markdown-syntax/issues"
  },
  "icon": "assets/images/logo.png",
  "license": "MIT",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  }
}
